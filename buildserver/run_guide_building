echo "Build nadamak guide in HTML and publish it  "
echo "--------------------------------------------"
echo `pwd`
cd `pwd`/docs/guide/en
make html
if [ $? -ne 0 ]; then
  echo "------------------------------------------"
  echo "Building Nadamak guid in HTML FAILED      "
  echo "------------------------------------------"
  exit 22
fi
